<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext
    https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
    http://www.liquibase.org/xml/ns/dbchangelog
    https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">

    <changeSet id="1.0.0_002_order_createTable" author="cgamache">
        <createTable tableName="ORDER">
            <column name="id" type="NUMBER(10,0)">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_ORDER"/>
            </column>
            <column name="productId" type="NUMBER(10,0)"/>
            <column name="quantity" type="NUMBER(5,0)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>